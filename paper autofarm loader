--// CONFIG
local CorrectKey = "deal"
local KeyLink = "https://link-target.net/1104136/CzoYylsJ7CL0"
local DiscordLink = "https://discord.gg/uqkvDaX3kH"

--// SERVICES
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

--// GUI
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Parent = game.CoreGui

local Frame = Instance.new("Frame")
Frame.Size = UDim2.new(0, 320, 0, 230)
Frame.Position = UDim2.new(0.5, -160, 0.5, -115)
Frame.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
Frame.BorderSizePixel = 0
Frame.Parent = ScreenGui

local TextBox = Instance.new("TextBox")
TextBox.Size = UDim2.new(0, 280, 0, 40)
TextBox.Position = UDim2.new(0.5, -140, 0, 20)
TextBox.PlaceholderText = "Enter Key"
TextBox.Text = ""
TextBox.TextScaled = true
TextBox.Parent = Frame

local Submit = Instance.new("TextButton")
Submit.Size = UDim2.new(0, 280, 0, 40)
Submit.Position = UDim2.new(0.5, -140, 0, 70)
Submit.Text = "Submit Key"
Submit.TextScaled = true
Submit.Parent = Frame

local GetKey = Instance.new("TextButton")
GetKey.Size = UDim2.new(0, 280, 0, 40)
GetKey.Position = UDim2.new(0.5, -140, 0, 120)
GetKey.Text = "Get Key"
GetKey.TextScaled = true
GetKey.Parent = Frame

-- ✅ DISCORD BUTTON
local GetDiscord = Instance.new("TextButton")
GetDiscord.Size = UDim2.new(0, 280, 0, 40)
GetDiscord.Position = UDim2.new(0.5, -140, 0, 170)
GetDiscord.Text = "Join Discord"
GetDiscord.TextScaled = true
GetDiscord.Parent = Frame

--// SUBMIT KEY
Submit.MouseButton1Click:Connect(function()
    if TextBox.Text == CorrectKey then
        ScreenGui:Destroy()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/dracoonmacoc/RealScripts/refs/heads/main/paper%20autofarm"))()
    else
        Submit.Text = "Wrong Key!"
        task.wait(1)
        Submit.Text = "Submit Key"
    end
end)

--// GET KEY BUTTON
GetKey.MouseButton1Click:Connect(function()
    if setclipboard then
        setclipboard(KeyLink)
    end
    GetKey.Text = "Copied!"
    task.wait(1)
    GetKey.Text = "Get Key"
end)

-- ✅ DISCORD BUTTON FUNCTION
GetDiscord.MouseButton1Click:Connect(function()
    if setclipboard then
        setclipboard(DiscordLink)
    end
    GetDiscord.Text = "Discord Copied!"
    task.wait(1)
    GetDiscord.Text = "Join Discord"
end)
